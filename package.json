{
  "name": "c2-arrow-buttons",
  "version": "1.2.0",
  "description": "This module builds \"next\" and \"previous\" buttons and emits an update whenever the index changes.",
  "main": "arrow-buttons.js",
  "scripts": {
    "standalone": "browserify arrow-buttons.js --outfile standalone/arrow-buttons.js --standalone ArrowButtons --exclude jquery",
    "dist": "uglifyjs standalone/arrow-buttons.js --output standalone/arrow-buttons.min.js --comments",
    "build": "npm run standalone && npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheC2Group/arrow-buttons.git"
  },
  "keywords": [
    "arrow buttons"
  ],
  "author": "The C2 Group",
  "license": "MIT"
}
